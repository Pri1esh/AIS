  "admissionDocuments": {
    "heading": "Documents for Admission",
    "data": [
      {
        "subHeading": "During Form Submission",
        "description": [
          {
            "label": "Self attested photocopy of Birth Certificate which has the name of both parents mentioned on it."
          },
          {
            "label": "Self attested proof of residence (Ration Card/ Voter Id/ Electricity or Water Bill/ Tenant's Deed/ Aadhaar)."
          },
          {
            "label": "Two passport size photographs of the student/applicant."
          }
        ]
      },
      {
        "subHeading": "At the time of Admission",
        "description": [
          {
            "label": "Self attested Aadhaar Card photocopy of the student/applicant."
          },
          {
            "label": "Blood Group report of the child."
          },
          {
            "label": "Medical report/Medical history/ Vaccination record of the child."
          },
          {
            "label": "Attested copies of legal document if the child is being brought up by a single parent."
          },
          {
            "label": "Affidavit of guardianship to be submitted if student lives with a local guardian. (Extra enclosures for Grades other than Grade Nursery to 1)",
            "subDescription": [
              {
                "label": "Conduct/Bonafide Certificate."
              },
              {
                "label": "Marksheet/Report Card of the student (For Grades 2 and above)"
              }
            ]
          }
        ]
      },
      {
        "subHeading": "After admission",
        "description": [
          {
            "label": "Original Transfer Certificate from the last school.  (To be Submitted within 15 days of admission, or else the admission stands cancelled)"
          }
        ]
      }
    ]
  },
